        "import {statSync, writeFileSync} from 'fs';\nimport {sync} from 'glob';\nimport {join} from 'path';\nimport {__, compose, curry, difference, filter, join as arrJoin, intersection, last, map, merge, partialRight, split, union} from 'ramda';\n\nimport {BRAND_FEATURES, MANIFEST, URBN_FEATURES} from '../config';\n\n/**\n * Manifest Feature Model\n * @private\n * @type {Object}\n */\nexport const manifestFeatureModel = {\n    package: '',\n    enabled: true,\n    client: {}\n};\n\n/**\n * A feature model\n * @private\n * @param  {String} project Name of the project i.e. anweb or urbnweb\n * @param  {String} name    Name of the feature\n * @return {Object}         Feature Info Model\n */\nfunction getManifestFeatureModel (project, name) {\n    return merge(manifestFeatureModel, {\n        package: arrJoin('.', [project, 'features', name])\n    });\n};\n\n// anweb and urbnweb feature models\nconst anwebFeatureInfo = curry(getManifestFeatureModel)('anweb');\nconst urbnwebFeatureInfo = curry(getManifestFeatureModel)('urbnweb');\n\n/**\n * Is the given path a directory?\n * @param  {String}  node\n * @private\n * @return {Boolean}      True, if a directory; false, otherwise.\n */\nfunction isDir (path) {\n    return statSync(path).isDirectory();\n}\n\n/**\n * Glob the contents of a directory (leaf) one-level deep\n * @param {String} path A directory path\n * @return {String} Single-level glob string\n */\nconst leafGlob = partialRight(join, ['*']);\n\n/**\n * Parse the feature names from a feature path\n * @private\n * @param {String} Feature path\n * @type {String} Feature name\n */\nconst parseFeatureName = compose(\n    last,\n    split('/')\n);\n\n// get rid of __pycache and core features\n// as both will automatically be required\nconst rejectFeatureNames = curry(difference)(\n    __,\n    ['__pycache__', 'core']\n);\n\n/**\n * Glob the contents of an Ancom feature\n * directory (leaf) one-level deep\n * @private\n * @param {String} path A directory path\n * @re