        "import webpack from 'webpack';\nimport {join, resolve} from 'path';\nimport {BRAND_FEATURES} from './config';\nimport {brandFeatureNames} from './utils';\nimport ngAnnotatePlugin from 'ng-annotate-webpack-plugin';\n\nconst RESOLVED_BRAND_FEATURES = resolve(BRAND_FEATURES);\nconst ENABLED_FEATURES = brandFeatureNames();\n\nfunction makeEntryMappings(features) {\n  let obj = {};\n\n  features.forEach(feature => {\n    obj[feature] = join(feature, 'index.js');\n  });\n\n  return obj;\n}\n\nfunction makeResolveAliases(features) {\n  let obj = {};\n\n  features.forEach(feature => {\n    obj[feature] = join(RESOLVED_BRAND_FEATURES, feature, 'js');\n  });\n\n  return obj;\n}\n\nconst webpackCfg = {\n  context: RESOLVED_BRAND_FEATURES,\n  resolve: {\n    root: RESOLVED_BRAND_FEATURES,\n    alias: makeResolveAliases(ENABLED_FEATURES)\n  },\n  entry: makeEntryMappings(ENABLED_FEATURES),\n  output: {\n    path: join(__dirname, 'anweb', 'dist', 'js'),\n    filename: '[name].js',\n    sourceMapFilename: '[name].map'\n  },\n  module: {\n    loaders: [\n      {\n        test: /\\.jsx?$/,\n        exclude: /(dist|node_modules|bower_components)/,\n        loaders: [\n          'babel?cacheDirectory=true&presets[]=es2015'\n        ],\n      }\n    ]\n  },\n  plugins: [\n    new ngAnnotatePlugin(),\n    // features/core/js/example.js is automatically detected as common module and extracted\n    new webpack.optimize.CommonsChunkPlugin('common.js', ENABLED_FEATURES)\n  ]\n};\n\nexport default webpackCfg;\n"
