        "/**\n * Gulp task for eslinting the JavaScript codebase.\n */\n\nimport {join} from 'path';\nimport gulp from 'gulp';\nimport {getFeaturePaths} from '../utils';\nimport cache from 'gulp-cached';\nimport eslint from 'gulp-eslint';\nimport ifFile from 'gulp-if';\nimport through2 from 'through2';\nimport formatterFriendly from 'eslint-friendly-formatter';\nimport formatterFiles from 'eslint-onelineperfile';\nimport formatterSummary from 'eslint-summary';\nimport formatterStats from 'eslint-stats';\nimport formatterTap from 'eslint-tap';\n\nfunction isLinty(file) {\n    // check if a file has any errors\n    return file.eslint && file.eslint.errorCount > 0;\n}\n\nfunction uncache(cacheName) {\n    // create a stream that removes files from cache\n    return through2.obj((file, enc, done) => {\n        if (cache.caches[cacheName]) {\n            delete cache.caches[cacheName][file.path];\n        }\n        done(null, file);\n    });\n}\n\nfunction formatterQuiet(reports) {\n    // count & remove warning messages from each eslint file report\n    reports.forEach((report) => {\n        report.messages = report.messages.filter(\n            (message) => message.fatal || message.severity > 1);\n    });\n    // The returned value goes to the console (by default)\n    // If nothing is returned, nothing is written.\n    return 'eslint:  VERBOSE=1 to see warnings, FORMAT=friendly|files|summary|stats|folders for reports';\n}\n\nfunction createEslintStream(sourceStream) {\n    let formatter;\n    let lintStream = sourceStream\n        .pipe(eslint());\n\n    switch (process.env.FORMAT) {\n    case 'friendly':\n        formatter = formatterFriendly;\n        break;\n    case 'files':\n        formatter = formatterFiles;\n        break;\n    case 'summary':\n        formatter = formatterSummary;\n        break;\n    case 'stats':\n        formatter = formatterStats.byErrorAndWarning;\n        break;\n    case 'folders':\n        formatter = formatterStats.byFolder;\n      