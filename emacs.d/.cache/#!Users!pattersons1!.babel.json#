        "import {join} from 'path';\nimport {curry} from 'ramda';\nimport gulp from 'gulp';\nimport {BRAND_FEATURES} from '../config';\nimport {getFeaturePaths} from '../utils';\nimport mergeStreams from 'merge-stream';\nimport spritesmith from 'gulp.spritesmith';\nimport svgSprite from 'gulp-svg-sprite';\nimport plumber from 'gulp-plumber';\n// import bust from 'gulp-buster';\n\n/**\n * Produce a PNG config object for a given feature\n * @param {String} feature The feature name\n * @return {Object} A PNG sprite configuration object\n */\nfunction createPngConfig(feature) {\n    const prefix = 'sprite-';\n\n    return {\n        imgName: prefix + feature + '.png',\n        imgPath: join('..', 'images', prefix + feature + '.png'),\n        cssFormat: 'scss_maps',\n        cssName: '_sprites-png.scss',\n        cssOpts: {\n            functions: false\n        },\n        cssSpritesheetName: prefix + feature,\n        cssVarMap: function (sprite) {\n            sprite.name = prefix + feature + '__' + sprite.name;\n        }\n    };\n}\n\n/**\n * Produce a SVG config object for a given feature\n * @param {String} feature The feature name\n * @return {Object} A SVG sprite configuration object\n */\nfunction createSvgConfig(feature) {\n    const prefix = 'sprite-';\n\n    return {\n        // log: 'debug',\n        mode: {\n            view: {\n                dest: 'scss',\n                sprite: join('..', 'images', prefix + feature + '.svg'),\n                bust: false,\n                common: prefix + feature,\n                mixin: prefix + feature,\n                render: {\n                    scss: {\n                        dest: join(feature, 'variables', '_sprites-svg.scss'),\n                        // the sprite template file is always\n                        // features/core/scss/sprites/sprite-svg-template.scss\n                        template: join(BRAND_FEATURES, 'core', 'scss', 'core', 'utilities', 'sprite-svg-template.hbs')\n                    }\n                }\n            },\n            symbol: {\n                dest: 'images',\n                sprite: prefix + feature + '.symbol.svg',\n                bust: false,\n                inline: true,\n                example: {\n                    dest: prefix + feature + '-example.html'\n                }\n            }\n        }\n    };\n}\n\n/**\n * Produce a PNG stream via gulp.spritesmith for a feature\n * @param {String} featurePath Root path of the feature\n * @param {String} filesPath Path to the image files of the feature\n * @param {Object} spriteConfig PNG sprite config object for gulp.spritesmith\n */\nfunction createPngPipeline(featurePath, filesPath, spriteConfig) {\n    return gulp.src(filesPath)\n        .pipe(plumber())\n        .pipe(spritesmith(spriteConfig, (err, result) => {\n            if (err) {\n                throw new Error(err);\n            }\n            console.info('Processed ' + result.coordinates.filepath);\n        })).on('error', console.log.bind(console));\n}\n\n/**\n * Produce a SVG stream via gulp-svg-sprite for a feature\n * @param {String} featurePath Root path of the feature\n * @param {String} filesPath Path to the image files of the feature\n * @param {Object} spriteConfig SVG sprite config object for gulp-svg-sprite\n */\nfunction createSvgPipeline(featurePath, filesPath, spriteConfig) {\n    return gulp.src(filesPath)\n        .pipe(plumber())\n        .pipe(svgSprite(spriteConfig)).on('error', console.log.bind(console))\n        .pipe(gulp.dest(featurePath));\n}\n\n/**\n * Pipe the CSS and Images streams to their respective destinations,\n * and merge all of the streams together.\n * @param {Object} stream The combined stream\n * @param {Object} sprite The sprite stream\n * @return {Object} The updated combined stream\n */\nfunction addPngStreams(stream, sprite) {\n    stream.add(sprite.pipeline.img\n        .pipe(gulp.dest(sprite.imagesPath)));\n\n    stream.add(sprite.pipeline.css\n        .pipe(gulp.dest(sprite.scssPath)));\n\n    return stream;\n}\n\n/**\n * Pipe the CSS and Images streams to their respective destinations,\n * and merge all of the streams together.\n * @param {Object} stream The combined stream\n * @param {Object} sprite The sprite stream\n * @return {Object} The updated combined stream\n */\nfunction addSvgStreams(stream, sprite) {\n    stream.add(sprite.pipeline);\n    return stream;\n}\n\n/**\n * Curried function to produce an array of streams, one per sprite file tree\n * @param {String} filePath The path of a PNG file\n * @return {Object} imagesPath: The images path\n *                  scssPath: The SCSS path\n *                  featurePath: The feature path\n *                  spriteData: A gulp stream that takes the filePath as a\n *                              source and pipes it to the computed spritesmith\n *                              configuration\n */\nconst createSpriteStreams = curry((createConfig, createPipeline, filePath) => {\n    const segments = filePath.split('/');\n    const featurePath = segments.slice(0, segments.length - 3).join('/');\n    const imagesPath = segments.slice(0, segments.length - 2).join('/');\n    const featureName = segments[segments.length - 4];\n    const scssPath = imagesPath.replace(/images/, 'scss/' + featureName + '/variables');\n    const spriteConfig = createConfig(featureName);\n\n    return {\n        imagesPath: imagesPath,\n        scssPath: scssPath,\n        featurePath: featurePath,\n        pipeline: createPipeline(featurePath, filePath, spriteConfig)\n    };\n});\n\n// create createStreams functions preloaded with the transform objects we need\nconst createPngStreams = createSpriteStreams(createPngConfig, createPngPipeline);\nconst createSvgStreams = createSpriteStreams(createSvgConfig, createSvgPipeline);\n\n\n// task definitions\n\nexport const spritesPngTask = gulp.task('sprites:png', () => {\n    return getFeaturePaths('images/sprites/*.png')\n        .map(createPngStreams)\n        .reduce(addPngStreams, mergeStreams());\n});\n\nexport const spritesSvgTask = gulp.task('sprites:svg', () => {\n    return getFeaturePaths('images/sprites/*.svg')\n        .map(createSvgStreams)\n        .reduce(addSvgStreams, mergeStreams());\n});\n\nexport const spritesTask = gulp.task('sprites', [\n    'clean:sprites',\n    'sprites:png',\n    'sprites:svg'\n]);\n\nexport const spritesWatchTask = gulp.task('sprites:watch', () => {\n    gulp.watch(getFeaturePaths('images/sprites/*.png'), ['sprites:png']);\n    gulp.watch(getFeaturePaths('images/sprites/*.svg'), ['sprites:svg']);\n});\n"
