'use strict';

// TODO: pull these values into HomepageConfig
var BASE_WIDTH = 596;
var RESPONSIVE_BREAKPOINT = 990;

module.exports = /*@ngInject*/
function landingPageController(
    $log,
    $rootScope,
    $routeParams,
    $scope,
    $location,
    $window,
    $rootElement,
    ContentService,
    ProfileHTTPService,
    ResponsiveService,
    ContentConfig,
    Anthro
) {
    // Model
    var m = this.model = $scope.model = {
        fields: {},
        pageOverride: false,
        partialContext: Anthro.location.root,
        moduleContext: Anthro.location.contextPath
    };

    // Actions
    var a = $scope.action = {
        /**
         * Persist the list of modules to the model
         * @param {Object} pageEntry Contentful entry object resolved by the LandingPage service
         */
        handlePageEntry: function handlePageEntry(pageEntry) {
            try {
                m.fields = pageEntry.fields;
                m.lcs = extractFields(pageEntry.fields.leadContentSlot);
                m.banner = extractFields(pageEntry.fields.bannerSlot);
            } catch(err) {
                a.overridePageEntry(err);
            }

            function extractFields(e) {
                if (e && e.fields) {
                    return {
                        clickThroughUrl: e.fields.clickThroughUrl,
                        openNewWindow: e.fields.openNewWindow,
                        largeImg: e.fields.largeImg ? e.fields.largeImg.fields.file.url : undefined,
                        mediumImg: e.fields.mediumImg ? e.fields.mediumImg.fields.file.url : undefined,
                        smallImg: e.fields.smallImg ? e.fields.smallImg.fields.file.url : undefined,
                        video: e.fields.video ? e.fields.video.fields.file.url : undefined
                    };
                } else {
                    // Undefined, not empty dictionary so that the template can hide based on
                    //  truthiness of the field
                    return undefined;
                }
            }
        },

        /**
         * Activate the LandingPage override flag if a Contentful data rejection occurs
         * @param {String} err The rejection message
         */
        overridePageEntry: function overridePageEntry(err) {
            // when pageEntry is null, load page override
            m.pageOverride = true;
            $log.warn(err);
        },

        /**
         * Checks to see if the content should be hidden on desktop or mobile
         * @param   {Object}  fields Contentful object of all fields
         * @returns {Boolean}        true indicates the content is hidden; false indicates the content is shown
         */
        isHidden: function isHidden(fields) {
            return (
                fields.hiddenOnDesktop && ResponsiveService.getWindowWidth() >= RESPONSIVE_BREAKPOINT ||
                fields.hiddenOnMobile && ResponsiveService.getWindowWidth() < RESPONSIVE_BREAKPOINT
            );
        }
    };

    /**
    * Initialize the Landing Page
    *
    * The page is loaded via 
    * 
    *  1 - "category-id" attribute on the root element
    *
    *  2 - $scope.pageControllerId - child controllers may override this (see HomepageController)
    *
    */
    var init = $scope.init = function init() {
        // The category injected from the jsp
        var currentCategory = ContentService.getCurrentCategoryId();

        ProfileHTTPService.getProfile().then(function onProfileRes (res) {
            var options = {
                'preferredLanguage' : res.customer.preferredLanguage,
                'overrideEntryId' : $location.search().contentid || $location.search().id,
                'pageContentTypeName' : ContentConfig.categoryContentTypeName,
                'categoryId' : currentCategory
            };

            return ContentService.getPageEntry(options);
        }).then(a.handlePageEntry, a.overridePageEntry);
    };

    init();
};


